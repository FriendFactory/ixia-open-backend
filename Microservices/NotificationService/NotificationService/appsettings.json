{
  "AllowedHosts": "*",
  "Auth": {
    "ApiName": "friends_factory.creators_api",
    "AuthServer": "http://localhost:5002/auth"
  },
  "AWS": {
    "Region": "eu-central-1",
    "Profile": "friendsfactory"
  },
  "BasePath": "/notification",
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "OneSignal": {
    "AppId": "xxxxxxxxx",
    "ApiKey": "xxxxxxxxx"
  },
  "Redis": {
    "ConnectionString": "localhost:6379",
    "EnableCaching": true,
    "ClientIdentifier": "local.notification-server"
  },
  "Services": {
    "Video": "http://localhost:5005/video",
    "Main": "http://localhost:5001/main"
  }
}